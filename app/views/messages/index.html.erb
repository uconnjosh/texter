
<span class="test">HELLO</s
  <div class="messages">
    <h1>Your Texts:</h1>
<br />
<br />
    <ul>
    <% @messages.all.each do |message| %>
    <li><%= message.body %></li>
    <a href="messages/<%= message.id %>" data-confirm="Are you sure?" data-method="delete" rel="nofollow">(delete)</a>
    <% end %>
    </ul>
    <br />
    <br />
    <%= link_to "Make a New Text", new_message_path(@message), remote: true, class: 'btn btn-primary', id: 'new_text' %>

    <br />

    <!-- <div id="create_text">
    </div> -->
  </div>

  <div class="numbers">
    <h1>Your Phone Numbers</h1>
    <br />
    <ul>
     <% @numbers.each do |number| %>
       <li><%= link_to "Send a text to: ", new_message_path(to: number.phone_number), remote: true, class: 'new_digit', :data => number.phone_number %> <%= number.contact_name %>&nbsp;<span class="digits", data="<%= number.phone_number %>"></span><%= number.phone_number %>&nbsp; </li>
     <% end %>
    <br />
    <%= link_to "Create a new contact", new_number_path(@number), remote: true, class:
    'btn btn-primary', id: 'new_number' %>
  </div>




